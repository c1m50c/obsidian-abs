{
  "arrowParens": "always",
  "trailingComma": "all",
  "singleQuote": false,
  "endOfLine": "lf",
  "tabWidth": 4,
  "semi": true,
  "useTabs": false,
  "overrides": [
    {
      "files": [
        "**/*.json",
        "**/*.yaml",
        "**/*.yml",
        ".prettierrc"
      ],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}